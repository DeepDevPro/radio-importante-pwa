commands:
  01_node_version:
    command: |
      echo "Node.js version info:"
      node --version
      npm --version
      echo "Package.json engines:"
      cat package.json | grep -A 5 "engines"
      echo "Environment variables:"
      env | grep NODE
      echo "Current working directory:"
      pwd
      echo "Files in current directory:"
      ls -la
